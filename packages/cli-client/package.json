{
  "name": "pxyz-client",
  "version": "1.0.2",
  "description": "Generate HTTP clients from OpenAPI specs",
  "keywords": [
    "protocol",
    "pxyz"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/protoxyz/pxyz-js.git",
    "directory": "packages/cli-client"
  },
  "license": "MIT",
  "author": {
    "name": "Eric Campbell",
    "url": "https://pxyz.dev"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist && rimraf .turbo && rimraf node_modules",
    "dev": "IS_PROTOCOL_DEV=true tsup --watch",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "gen:backend": "pnpm start:dev  gen:client @protoxyz/client-backend http://127.0.0.1:4000/docs/swagger.json -o ../client-backend ",
    "gen:frontend": "pnpm start:dev  gen:client @protoxyz/client-frontend https://api.ericc59.pxyz.dev/frontend/openapi.json -o ../client-frontend ",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm build && pnpm publish --access public",
    "release": "changeset version",
    "start": "node dist/index.js",
    "start:dev": "IS_PROTOCOL_DEV=true node dist/index.js",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.4",
    "@babel/core": "^7.22.1",
    "@babel/parser": "^7.22.6",
    "@babel/plugin-transform-typescript": "^7.22.5",
    "@protoxyz/core": "workspace:*",
    "@protoxyz/types": "workspace:*",
    "chalk": "5.2.0",
    "commander": "^10.0.0",
    "cosmiconfig": "^8.1.3",
    "diff": "^5.1.0",
    "execa": "^5.1.1",
    "fs-extra": "^11.1.0",
    "https-proxy-agent": "^6.2.0",
    "js-beautify": "^1.14.3",
    "lodash.template": "^4.5.0",
    "node-fetch": "^3.3.0",
    "ora": "^6.1.2",
    "prettier": "^2.8.3",
    "prompts": "^2.4.2",
    "recast": "^0.23.2",
    "ts-morph": "^18.0.0",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.1",
    "@types/diff": "^5.0.3",
    "@types/fs-extra": "^11.0.1",
    "@types/js-beautify": "^1.14.3",
    "@types/lodash.template": "^4.5.1",
    "@types/prompts": "^2.4.2",
    "rimraf": "^4.1.3",
    "tsup": "^6.6.3",
    "type-fest": "^3.8.0",
    "typescript": "^4.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
