{
  "name": "@protoxyz/auth",
  "version": "1.0.18",
  "private": false,
  "homepage": "https://pxyz.dev",
  "bugs": {
    "url": "https://github.com/protoxyz/pxyz-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/protoxyz/pxyz-js.git",
    "directory": "packages/auth"
  },
  "license": "MIT",
  "author": {
    "name": "Eric Campbell",
    "url": "https://pxyz.dev"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./client/index.d.ts",
        "default": "./client/index.js"
      },
      "require": {
        "types": "./client/index.d.cts",
        "default": "./client/index.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "client"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@protoxyz/core": "workspace:*",
    "@protoxyz/themes": "workspace:*",
    "@protoxyz/types": "workspace:*",
    "jose": "^4.12.0",
    "js-cookie": "^3.0.5",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@protoxyz/config-tsconfig": "github:protoxyz/config-tsconfig",
    "@protoxyz/eslint-config-custom-next": "github:protoxyz/eslint-config-custom-next",
    "@types/js-cookie": "^3.0.5",
    "@types/node": "^18.14.2",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.1",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^7.1.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "next": ">=13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
