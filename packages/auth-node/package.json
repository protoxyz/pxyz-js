{
    "name": "@protoxyz/auth-node",
    "version": "1.0.1",
    "license": "MIT",
    "private": false,
    "homepage": "https://pxyz.dev",
    "repository": {
        "type": "git",
        "url": "https://github.com/protoxyz/pxyz-js.git"
    },
    "bugs": {
        "url": "https://github.com/protoxyz/pxyz-js/issues"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --minify --dts",
        "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
        "clean": "rm -rf .turbo node_modules dist",
        "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@protoxyz/core": "*",
        "@protoxyz/types": "*",
        "encoding": "^0.1.13",
        "jsonwebtoken": "^8.5.1"
    },
    "devDependencies": {
        "@protoxyz/config-tsconfig": "git@github.com:protoxyz/config-tsconfig.git",
        "@protoxyz/eslint-config-custom-library": "git@github.com:protoxyz/eslint-config-custom-library.git",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/node": "^18.14.2",
        "@types/node-fetch": "2.x",
        "eslint": "^8.32.0",
        "eslint-config-next": "^13.1.1",
        "node-fetch": "2.x",
        "tsup": "^6.2.3",
        "typescript": "^4.9.5"
    }
}
