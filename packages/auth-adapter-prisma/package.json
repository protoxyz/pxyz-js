{
  "name": "@abhub/auth-adapter-prisma",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules dist ./prisma/dev.db* || echo 'File deleted'",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "format:fix": "prettier --write . --ignore-path ../../.gitignore",
    "lint": "eslint src/",
    "typecheck": "tsc -noEmit",
    "init:db": "prisma migrate dev --name init --skip-seed",
    "test": "pnpm init:db && vitest -c ../shared/src/vitest.config.ts",
    "build": "prisma generate && tsc",
    "dev": "prisma generate && tsc -w",
    "studio": "prisma studio"
  },
  "peerDependencies": {
    "@prisma/client": ">=2.26.0 || >=3 || >=4 || >=5"
  },
  "devDependencies": {
    "@abhub/eslint-config": "workspace:^0.2.0",
    "@abhub/prettier-config": "workspace:^0.1.0",
    "@abhub/tsconfig": "workspace:^0.1.0",
    "@prisma/client": "^5.8.1",
    "@prisma/extension-accelerate": "^0.6.2",
    "@types/node": "^20.10.6",
    "@vitest/coverage-v8": "^1.2.2",
    "dotenv-cli": "^7.3.0",
    "next": "^14.1.0",
    "prisma": "^5.8.1",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@abhub/auth-lib": "workspace:*",
    "@abhub/config": "workspace:*",
    "zod": "^3.22.4"
  }
}
