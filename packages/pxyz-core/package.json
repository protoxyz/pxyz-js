{
    "name": "@protoxyz/core",
    "version": "1.0.5",
    "license": "MIT",
    "private": false,
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --minify --dts",
        "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
        "clean": "rm -rf .turbo node_modules dist",
        "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
        "type-check": "tsc --noEmit"
    },
    "devDependencies": {
        "@protoxyz/config-tailwind": "git@github.com:protoxyz/config-tailwind.git",
        "@protoxyz/config-tsconfig": "git@github.com:protoxyz/config-tsconfig.git",
        "@protoxyz/eslint-config-custom-library": "git@github.com:protoxyz/eslint-config-custom-library.git",
        "@types/node": "^18.14.2",
        "@types/node-fetch": "2.x",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "abort-controller": "3.x",
        "eslint": "^8.32.0",
        "form-data": "4.x",
        "install": "^0.13.0",
        "node-fetch": "2.x",
        "tsup": "^6.2.3",
        "typescript": "^4.9.5"
    }
}
