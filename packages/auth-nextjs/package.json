{
    "name": "@protoxyz/auth-nextjs",
    "version": "1.0.0",
    "license": "MIT",
    "private": false,
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --minify --dts",
        "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
        "clean": "rm -rf .turbo node_modules dist",
        "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
        "type-check": "tsc --noEmit"
    },
    "devDependencies": {
        "@protoxyz/config-tsconfig": "git@github.com:protoxyz/config-tsconfig.git",
        "@protoxyz/eslint-config-custom-next": "git@github.com:protoxyz/eslint-config-custom-next.git",
        "@types/node": "^18.14.2",
        "eslint": "^8.32.0",
        "eslint-config-next": "^13.1.1",
        "tsup": "^6.2.3",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "@protoxyz/core": "*",
        "jose": "^4.12.0",
        "next": "^13.3.0"
    }
}
